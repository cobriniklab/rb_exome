@article{sethPreexistingFunctionalHeterogeneity2019,
	title = {Pre-existing {Functional} {Heterogeneity} of {Tumorigenic} {Compartment} as the {Origin} of {Chemoresistance} in {Pancreatic} {Tumors}},
